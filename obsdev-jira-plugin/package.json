{
  "name": "obsdevjiraplugin",
  "description": "A plugin for obsdev that uploads saved videos to JIRA ticket",
  "version": "1.0.0",
  "author": "Chris Fields II",
  "bin": "./bin/run.js",
  "types": "dist/index.d.ts",
  "bugs": "https://github.com/projects/obsdevjiraplugin/issues",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "repository": "projects/obsdevjiraplugin",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/projects/obsdevjiraplugin",
  "keywords": [
    "oclif"
  ],
  "oclif": {
    "bin": "obsdevjiraplugin",
    "dirname": "obsdevjiraplugin",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {},
    "hooks": {
      "videoSaved": "./dist/hooks/jira"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  }
}
